<%= simple_form_for @report do |f| %>
  <%= f.input :name %>
  <%= f.input :quantity, as: :number %>
  <%= f.input :address %>
  <%= f.input :latitude, as: :decimal %>
  <%= f.input :longitude, as: :decimal %>
  <%= f.input :photo, as: :file %>
  <%= f.input :description, as: :text %>
  <%= f.button :submit %>
<% end %>
